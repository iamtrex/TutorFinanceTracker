<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>

<HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0" spacing="10.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.rweqx.controller.StudentProfileClassItemController">
   <children>
      <Label fx:id="lEventType" text="Class" />
      <Label fx:id="lDate" text="Label" />
      <Label fx:id="lDuration" text="Label" />
      <Label fx:id="lClassType" text="Label" />
      <Pane HBox.hgrow="ALWAYS" />
      <Button fx:id="bEdit" mnemonicParsing="false" onAction="#editClicked" text="Edit" />
   </children>
   <padding>
      <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
   </padding>
</HBox>
