<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>


<HBox alignment="CENTER" prefHeight="400.0" prefWidth="600.0" spacing="10.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.rweqx.controller.StudentProfileEventItemController">
   <children>
      <Label fx:id="lType" styleClass="smaller-viewing-label" stylesheets="@../styles/style.css" />
      <Label fx:id="lDuration" styleClass="smaller-viewing-label" stylesheets="@../styles/style.css" />
      <Label fx:id="lPaid" styleClass="smaller-viewing-label" stylesheets="@../styles/style.css" />
      <Pane HBox.hgrow="ALWAYS" />
      <Button fx:id="bEdit" mnemonicParsing="false" styleClass="clean-button" stylesheets="@../styles/style.css" text="Edit" />
   </children>
   <padding>
      <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
   </padding>
</HBox>
